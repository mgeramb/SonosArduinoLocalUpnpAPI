    "description": "Sonos-ESP inspired successor library for controlling Sonos speakers over the unofficial local UPNP API",
    "export": {
        "exclude": [
            "test"
        ]
    },
    "frameworks": "arduino",
    "keywords": "sonos, upnp, client, audioclip",
    "license": "GPL",
    "name": "SonosArduinoLocalUpnpAPI",
    "platforms": "espressif32",
    "repository": {
        "type": "git",
        "url": "https://github.com/mgeramb/SonosArduinoLocalUpnpAPI.git"
    },
    "version": "0.0.8",
    "dependencies": {
	    "ESP Async WebServer": "ESP Async WebServer @ ^2.7.0",
	    "MicroXPath": "https://github.com/tmittet/microxpath.git @ ^1.1.0",
	    "WebSockets": "https://github.com/lemonkey/arduinoWebSockets.git @ ^2.3.7"
    },
    "build": {
        "libLDFMode": "deep"
    }
}